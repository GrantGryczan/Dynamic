{
	"name": "Dynamic",
	"version": "0.20.23",
	"description": "Create HTML5 animations with ease!",
	"main": "app/main.js",
	"scripts": {
		"start": "electron . dev",
		"build": "node ./build/build.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GrantGryczan/web.git"
	},
	"author": {
		"name": "Grant Gryczan",
		"email": "grantgryczan@gmail.com"
	},
	"build": {
		"appId": "io.miroware.dynamic",
		"productName": "Miroware Dynamic",
		"fileAssociations": [
			{
				"ext": "mwd",
				"name": "Miroware Dynamic File",
				"description": "Miroware Dynamic File"
			}
		],
		"mac": {
			"category": "public.app-category.utilities"
		},
		"win": {
			"target": "nsis"
		},
		"nsis": {
			"perMachine": true,
			"uninstallDisplayName": "Miroware Dynamic"
		},
		"linux": {
			"synopsis": "Create HTML5 animations with ease!",
			"description": "Create HTML5 animations with ease!",
			"category": "Utility"
		}
	},
	"devDependencies": {
		"electron": "^2.0.7",
		"electron-builder": "^19.45.5"
	},
	"dependencies": {
		"fs-extra": "^6.0.1"
	}
}
